{
  "files.exclude": {
    "node_modules/**": true,
    "**/*.js": true,
    "**/*.js.map": true,
    "**/*.d.ts": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": false
  },
  "files.associations": {
    "*.iced": "coffeescript"
  }
}